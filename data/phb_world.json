e = {
    "exchange_rate" : {
        "cp" : {
            "cp" : 1,
            "sp" : 0.1,
            "ep" : 0.01,
            "gp" : 0.005,
            "pp" : 0.001 
        },
        "sp" : {
            "cp" : 10,
            "sp" : 1,
            "ep" : 0.1,
            "gp" : 0.05,
            "pp" : 0.01
        },
        "ep" : {
            "cp" : 100,
            "sp" : 10,
            "ep" : 1,
            "gp" : 0.5,
            "pp" : 0.1
        },
        "gp" : {
            "cp" : 200,
            "sp" : 20,
            "ep" : 2,
            "gp" : 1,
            "pp" : 0.2
        },
        "pp" : {
            "cp" : 1000,
            "sp" : 100,
            "ep" : 10,
            "gp" : 5,
            "pp" : 1
        }
    }
}
    
def exg( it, q, ot ):
    io = e['exchange_rate'][it][ot]
    return io * q

print( exg( 'pp', 100, 'cp' ) )

def frac_gp( money ):
    tot = 0
    for t in ('cp','sp','gp','ep','pp'):
        if t in money:
            tot = tot + exg( t, money[t], 'gp' )
    return tot

print( frac_gp( {'sp':10,'pp':5} ) )
